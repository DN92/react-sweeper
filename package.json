{
  "name": "react-nosock-boilerplate",
  "version": "1.0.0",
  "description": "React Express PG Sequelize boilerplate",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "npm run build -- --watch --mode=development",

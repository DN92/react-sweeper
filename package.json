    "express-session": "^1.17.3",
    "express-session-sequelize": "^2.3.0",
    "morgan": "^1.10.0",
    "node-process": "^1.0.1",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "pg": "^8.8.0",
